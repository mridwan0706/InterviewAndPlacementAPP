    <script src="~/plugins/styleswitcher/jQuery.style.switcher.js"></script>

    <script>


        $(document).ready(function () {
            debugger;
            tabel = $("#example23").DataTable({
                dom: 'Bfrtip',
                buttons: [
                    // 'copy', 'csv', 'excel', 'pdf', 'print'
                    'excel', 'pdf', 'print'
                ],
                "columnDefs": [{
                    "searchable": false,
                    "orderable": false,
                    "targets": 0
                }],
                "order": [[1, 'asc']]
            });
            tabel.on('order.dt search.dt', function () {
                tabel.column(0, { search: 'applied', order: 'applied' }).nodes().each(function (cell, i) {
                    cell.innerHTML = i + 1;
                });
            }).draw();
        });
    </script>

}

