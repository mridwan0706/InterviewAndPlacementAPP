
@{
    ViewData["Title"] = "Approve";
    Layout = "~/Views/Layout/Layout_Dashboard.cshtml";
}


<div class="container-fluid">
    <!-- ============================================================== -->
    <!-- Bread crumb and right sidebar toggle -->
    <!-- ============================================================== -->
    <div class="row page-titles">
        <div class="col-md-6 col-8 align-self-center">
            <h3 class="text-themecolor m-b-0 m-t-0">Interview</h3>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="javascript:void(0)">Home</a></li>
                <li class="breadcrumb-item active">Interview</li>
            </ol>
        </div>
        <div class="col-lg-12 col-4 align-self-center">
            <button class="right-side-toggle waves-effect waves-light btn-info btn-circle btn-sm pull-right m-l-10"><i class="ti-settings text-white"></i></button>
            <div class="nav-item dropdown mega-dropdown">
                <button class="btn pull-right btn-success waves-effect waves-dark" data-toggle="modal" data-target="#myModal"><i class="mdi mdi-plus-circle"></i> Create</button>

            </div>

            <div class="dropdown-menu mailbox animated bounceInDown" aria-labelledby="2">

            </div>

            <div class="dropdown pull-right m-r-10 hidden-sm-down">
                <button class="btn btn-secondary dropdown-toggle" type="button" id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> January 2017 </button>
                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton"> <a class="dropdown-item" href="#">February 2017</a> <a class="dropdown-item" href="#">March 2017</a> <a class="dropdown-item" href="#">April 2017</a> </div>
            </div>
        </div>
    </div>  

    <!-- END Modal -->
    <!-- ============================================================== -->
    <!-- End Bread crumb and right sidebar toggle -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- Start Page Content -->
    <!-- ============================================================== -->
    <div class="row">
        <div class="col-12">
            <div class="card">
                <div class="card-body">
                    <h4 class="card-title">Data Approver Interviews</h4>

                    <div class="table-responsive m-t-40">
                        <table id="example23" class="display nowrap table table-hover table-striped table-bordered" cellspacing="0" width="100%">
                            <thead>
                                <tr>
                                    <th>No.</th>
                                    <th>Name</th>
                                    <th>Interview Date</th>
                                    <th>Site Name</th>
                                    <th>Status</th>
                                    <th class="text-center">Action</th>
                                </tr>
                            </thead>
                            <tfoot>
                                <tr>
                                    <th>No</th>
                                    <th>Name</th>
                                    <th>Invitation Date</th>
                                    <th>Site Name</th>
                                    <th>Status</th>
                                    <th class="text-center">Action</th>
                                </tr>
                            </tfoot>
                            <tbody>
                                

                                    @foreach (var result in ViewBag.Result)
                                    {
                                     <tr>
                                         <td></td>
                                         <td>@result.EmployeeName</td>
                                         <td>@Convert.ToDateTime(result.InterviewDate).ToShortDateString()</td>
                                         <td>@result.SiteName</td>                                           
                                         <td><span class="label label-success text-center ml-lg-4" style="width:60%;">@result.Status</span></td>                                          
                                         <td class="text-center"><span class="label label-success text-center">Accept</span> | <span class="label label-danger text-center">Reject</span></td>
                                </tr>
                                }                                      
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- ============================================================== -->
    <!-- End PAge Content -->
    <!-- ============================================================== -->
    <!-- ============================================================== -->
    <!-- Right sidebar -->
    <!-- ============================================================== -->
    <!-- .right-sidebar -->
